{
	"manifest_version": 3,
	"name": "My Chrome Extension",
	"version": "1.0",
	"description": "A simple Chrome extension example.",
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": [ "storage", "tabs" ],
	"host_permissions": [ "https://openrouter.ai/*" ],
	"content_scripts": [
		{
			"matches": [ "https://*/*", "http://*/*" ],
			"js": [ "content.js" ]
		}
	]
}
